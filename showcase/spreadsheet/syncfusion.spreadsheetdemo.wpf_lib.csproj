<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>library</OutputType>
		<AssemblyName>syncfusion.spreadsheetdemo.wpf</AssemblyName>
		<RootNamespace>syncfusion.spreadsheetdemo.wpf</RootNamespace>
	</PropertyGroup>
	<ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
		<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
	</ItemGroup>
	<ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
		<Reference Include="PresentationCore" />
		<Reference Include="PresentationFramework" />
		<Reference Include="System" />
		<Reference Include="System.Data" />
		<Reference Include="System.Xml" />
		<Reference Include="Microsoft.CSharp" />
		<Reference Include="System.Core" />
		<Reference Include="System.Xml.Linq" />
		<Reference Include="System.Data.DataSetExtensions" />
		<Reference Include="System.Net.Http" />
		<Reference Include="System.Xaml">
		</Reference>
		<Reference Include="WindowsBase" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="Syncfusion.Compression.Base">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Compression.Base.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.ExcelChartToImageConverter.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.ExcelChartToImageConverter.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Licensing">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Licensing.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfCellGrid.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfCellGrid.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfChart.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfChart.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfGridCommon.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfGridCommon.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfSpreadsheet.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfSpreadsheet.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfSpreadsheetHelper.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfSpreadsheetHelper.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Shared.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Shared.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Tools.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Tools.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfSkinManager.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfSkinManager.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Themes.Office2019White.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Themes.Office2019White.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.XlsIO.Base">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.XlsIO.Base.dll</HintPath>
		</Reference>
		<PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
	</ItemGroup>
	<ItemGroup>
		<Content Include="Data\SpreadsheetShowCase\Showcase.xlsx">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
		<Resource Include="Assets\SfSpreadsheet.png" />
	</ItemGroup>
	<ItemGroup>
		<None Remove="packages.config" />
		<None Remove="App.config" />
		<Compile Remove="App.xaml.cs" />
		<ApplicationDefinition Remove="App.xaml" />
	</ItemGroup>
	<Import Project="..\..\targets\MultiTargeting.targets" />
</Project>